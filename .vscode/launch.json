    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\test\\cmockatest.exe",
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,            
            "launchCompleteCommand": "exec-run",
            "linux": {
              "MIMode": "gdb",
              "miDebuggerPath": "/usr/bin/gdb"
            },
            "osx": {
              "MIMode": "lldb"
            },
            "windows": {
              "MIMode": "gdb",
              "targetArchitecture": "x64",
              "miDebuggerPath": "C:\\cygwin64\\bin\\gdb.exe"
            }
          }
    ],
}